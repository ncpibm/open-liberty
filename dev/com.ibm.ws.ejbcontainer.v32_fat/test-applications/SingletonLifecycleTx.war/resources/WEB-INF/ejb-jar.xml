    </session>
    <session>
      <ejb-name>SingletonLifecycleTxRequiredXMLStyle3Bean</ejb-name>
      <ejb-class>com.ibm.ws.ejbcontainer.v32.fat.singletonlifecycletx.SingletonLifecycleTxNotSupportedXMLBean</ejb-class>
    </session>
    <session>
      <ejb-name>SingletonLifecycleTxRequiredXMLStyle3UnspecBean</ejb-name>
      <ejb-class>com.ibm.ws.ejbcontainer.v32.fat.singletonlifecycletx.SingletonLifecycleTxNotSupportedXMLBean</ejb-class>
