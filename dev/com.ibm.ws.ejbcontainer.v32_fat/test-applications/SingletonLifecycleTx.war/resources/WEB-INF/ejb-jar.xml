        <ejb-name>SingletonLifecycleTxRequiresNewXMLStyle3Bean</ejb-name>
        <method-intf>LifecycleCallback</method-intf>
        <method-name>postConstruct</method-name>
      </method>
      <method>
        <ejb-name>SingletonLifecycleTxRequiresNewXMLStyle3UnspecBean</ejb-name>
        <method-name>postConstruct</method-name>
      </method>
      <method>
        <ejb-name>SingletonLifecycleTxRequiresNewXMLStyle4Bean</ejb-name>
        <method-intf>LifecycleCallback</method-intf>
        <method-name>postConstruct</method-name>
        <method-params/>
      </method>
      <method>
        <ejb-name>SingletonLifecycleTxRequiresNewXMLStyle4UnspecBean</ejb-name>
        <method-name>postConstruct</method-name>
        <method-params/>
      </method>
      <trans-attribute>RequiresNew</trans-attribute>
    </container-transaction>

    <container-transaction>
      <method>
        <ejb-name>SingletonLifecycleTxNotSupportedXMLStyle1Bean</ejb-name>
        <method-name>*</method-name>
      </method>
      <method>
        <ejb-name>SingletonLifecycleTxNotSupportedXMLStyle2Bean</ejb-name>
        <method-intf>LifecycleCallback</method-intf>
        <method-name>*</method-name>
      </method>
      <method>
        <ejb-name>SingletonLifecycleTxNotSupportedXMLStyle3Bean</ejb-name>
        <method-intf>LifecycleCallback</method-intf>
        <method-name>postConstruct</method-name>
      </method>
      <method>
        <ejb-name>SingletonLifecycleTxNotSupportedXMLStyle3UnspecBean</ejb-name>
        <method-name>postConstruct</method-name>
      </method>
      <method>
        <ejb-name>SingletonLifecycleTxNotSupportedXMLStyle4Bean</ejb-name>
        <method-intf>LifecycleCallback</method-intf>
        <method-name>postConstruct</method-name>
        <method-params/>
      </method>
      <method>
        <ejb-name>SingletonLifecycleTxNotSupportedXMLStyle4UnspecBean</ejb-name>
        <method-name>postConstruct</method-name>
        <method-params/>
      </method>
      <trans-attribute>NotSupported</trans-attribute>
    </container-transaction>

    <container-transaction>
      <method>
        <ejb-name>SingletonLifecycleTxMandatoryXMLErrorBean</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Mandatory</trans-attribute>
    </container-transaction>
    <container-transaction>
      <method>
        <ejb-name>SingletonLifecycleTxNeverXMLErrorBean</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Never</trans-attribute>
    </container-transaction>
    <container-transaction>
      <method>
        <ejb-name>SingletonLifecycleTxSupportsXMLErrorBean</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Supports</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
