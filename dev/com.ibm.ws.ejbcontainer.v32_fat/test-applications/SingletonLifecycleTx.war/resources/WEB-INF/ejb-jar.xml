      <ejb-class>com.ibm.ws.ejbcontainer.v32.fat.singletonlifecycletx.SingletonLifecycleTxXMLBean</ejb-class>
    </session>
    <session>
      <ejb-name>SingletonLifecycleTxNotSupportedXMLStyle4Bean</ejb-name>
