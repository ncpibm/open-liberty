      <ejb-class>com.ibm.ws.ejbcontainer.v32.fat.singletonlifecycletx.SingletonLifecycleTxXMLBean</ejb-class>
    </session>
  </enterprise-beans>

  <assembly-descriptor>
    <container-transaction>
      <method>
        <ejb-name>SingletonLifecycleTxRequiredXMLStyle1Bean</ejb-name>
        <method-name>*</method-name>
      </method>
      <method>
        <ejb-name>SingletonLifecycleTxRequiredXMLStyle2Bean</ejb-name>
        <method-intf>LifecycleCallback</method-intf>
        <method-name>*</method-name>
      </method>
      <method>
        <ejb-name>SingletonLifecycleTxRequiredXMLStyle3Bean</ejb-name>
        <method-intf>LifecycleCallback</method-intf>
        <method-name>postConstruct</method-name>
      </method>
      <method>
        <ejb-name>SingletonLifecycleTxRequiredXMLStyle3UnspecBean</ejb-name>
        <method-name>postConstruct</method-name>
      </method>
      <method>
        <ejb-name>SingletonLifecycleTxRequiredXMLStyle4Bean</ejb-name>
        <method-intf>LifecycleCallback</method-intf>
        <method-name>postConstruct</method-name>
        <method-params/>
      </method>
      <method>
        <ejb-name>SingletonLifecycleTxRequiredXMLStyle4UnspecBean</ejb-name>
        <method-name>postConstruct</method-name>
        <method-params/>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>

    <container-transaction>
      <method>
        <ejb-name>SingletonLifecycleTxRequiresNewXMLStyle1Bean</ejb-name>
        <method-name>*</method-name>
      </method>
      <method>
        <ejb-name>SingletonLifecycleTxRequiresNewXMLStyle2Bean</ejb-name>
